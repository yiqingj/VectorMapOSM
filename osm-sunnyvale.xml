  <Layer name="sunnyvale-xml" srs="+proj=longlat +ellps=WGS84 +datum=WGS84 +no_defs">
    <StyleName>My Style</StyleName>
    <Datasource>
      <Parameter name="type">osm</Parameter>
      <Parameter name="file">sunnyvale.osm</Parameter>
    </Datasource>
  </Layer>
</Map>